{
  "name": "gTalk Syntax Highlighting",
  "version": "0.2.5",
  "manifest_version": 2,
  "description": "Automatic Syntax Highlighting for gChat.",
  "icons": { "16": "images/icon-16.png",
             "48": "images/icon-48.png",
            "128": "images/icon-128.png" },
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "css": ["stylesheets/main.css", "stylesheets/default.css"],
      "js": ["lib/jquery-1.9.0.min.js", "lib/underscore.js", "lib/util.js", "lib/highlight.min.js", "lib/classifier-0.1.0.min.js", "javascripts/classifier_info.js", "javascripts/gtalkSyntax.js", "javascripts/highlight.js"]
    }
  ],
  "permissions": [
    "https://mail.google.com/*",
    "storage"
  ],
  "options_page": "options.html"
}
